
pipeline {

    agent any

    stages {
        
        stage('Execute Jmeter') {
            steps {
                 bat("dir")
              bat("jmeter -n -t sample.jmx -l Samplereport.jtl")
            }
        }
    }
    
    post {
        always {
            // Publish performance test results
            publishHTML([$class: 'HtmlPublisher', reportDir: 'documents', reportFiles: 'test_results.html'])
           
        }
    }
}

